<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:interpolator="@android:anim/decelerate_interpolator">
    <translate
        android:duration="300"
        android:fromXDelta="0"
        android:toXDelta="-10%p"/>

    <translate
        android:duration="200"
        android:fromXDelta="-10%p"
        android:toXDelta="7%p"
        android:startOffset="300"/>

    <translate
        android:duration="140"
        android:fromXDelta="7%p"
        android:toXDelta="-5%p"
        android:startOffset="500"/>

    <translate
        android:duration="100"
        android:fromXDelta="-5%p"
        android:toXDelta="3%p"
        android:startOffset="640"/>

    <translate
        android:duration="60"
        android:fromXDelta="3%p"
        android:toXDelta="-1%p"
        android:startOffset="740"/>

    <translate
        android:duration="20"
        android:fromXDelta="-1%p"
        android:toXDelta="0"
        android:startOffset="800"/>
</set>